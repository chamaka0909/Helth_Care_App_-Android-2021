<?xml version="1.0" encoding="utf-8"?>


<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

 <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     >

        <Button
            android:id="@+id/sheduleex"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="25dp"
            android:background="@drawable/rounded"
            android:drawableLeft="@drawable/channnal"
            android:gravity="center"
            android:padding="20dp"
            android:text="Channeling"
            android:textAllCaps="false"
            android:textSize="21dp"
            android:textStyle="bold" />


        <Button
            android:id="@+id/llreport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="200dp"
            android:layout_marginRight="25dp"
            android:background="@drawable/rounded"
            android:drawableLeft="@drawable/labbb"
            android:gravity="center"
            android:padding="20dp"
            android:text="Lab Report"
            android:textAllCaps="false"
            android:textSize="21dp"
            android:textStyle="bold" />




     <Button
         android:id="@+id/uiui"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_marginLeft="25dp"
         android:layout_marginTop="375dp"
         android:layout_marginRight="25dp"
         android:background="@drawable/rounded"
         android:drawableLeft="@drawable/ppccrr"
         android:gravity="center"
         android:padding="20dp"
         android:text="PCR"
         android:textAllCaps="false"
         android:textSize="21dp"
         android:textStyle="bold" />



     <Button
         android:id="@+id/fitfit"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_marginLeft="25dp"
         android:layout_marginTop="550dp"
         android:layout_marginRight="25dp"
         android:background="@drawable/rounded"
         android:drawableLeft="@drawable/fitinn"
         android:gravity="center"
         android:padding="20dp"
         android:text="Fitness"
         android:textAllCaps="false"
         android:textSize="21dp"
         android:textStyle="bold" />


 </RelativeLayout>
    </ScrollView>
